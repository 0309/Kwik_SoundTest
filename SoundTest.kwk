<project id="SoundTest">  <settings>    <name>SoundTest</name>    <media>Book</media>    <device>New iPad</device>    <OS>iOS</OS>    <zoom>zoomStretch</zoom>    <width>2048</width>    <height>1536</height>    <orientation>Landscape</orientation>    <resolution>264</resolution>    <bookmark>false</bookmark>    <swipe>true</swipe>    <pinch>false</pinch>    <thumbs>false</thumbs>  </settings>  <pages>    <page id="page1">      <name>page1</name>      <number>1</number>      <file>~/Documents/Kwik/SoundTest/page1.psd</file>      <fileName>page1.psd</fileName>      <audio id="sheep-cry1">        <name>sheep-cry1</name>        <audio>~/Documents/Kwik/SoundTest/sheep-cry1.mp3</audio>        <delay>0</delay>        <loop>1</loop>        <isShort>true</isShort>        <allowRepeat>false</allowRepeat>        <isReadMe>false</isReadMe>        <keep>false</keep>        <autoPlay>false</autoPlay>        <lang>All</lang>        <channel>1</channel>        <volume>10</volume>        <fadeIn>false</fadeIn>        <fadeInAmount>.5</fadeInAmount>      </audio>      <button id="but_sheep">        <name>but_sheep</name>        <layer>sheep</layer>        <rollover>sheep</rollover>        <target>Layer</target>        <type>Tap</type>        <tap>1</tap>        <action id="play_865">          <name>play_865</name>          <type>Play Audio</type>          <audio>sheep-cry1</audio>        </action>      </button>    </page>  </pages></project>